#include<iostream>
using namespace std;
int matrix(int m,int n)
{
    int a[m][n];
    for (int i=0;i<m;i++)
    {
        for (int j=0;j<n;j++)
        {
            a[i][j]=rand()%10;
        }
    }
    cout<<"Original Matrix:\n";
    for (int i=0;i<m;i++)
    {
        for (int j=0;j<n;j++)
        {
            cout<<a[i][j]<<"\t";
        }
        cout<<"\n";
    }
    cout<<"\n";
    cout<<"Transposed Matrix:\n";
    int b[n][m];
    for (int i=0;i<n;i++)
    {
        for (int j=0;j<m;j++)
        {
            b[i][j]=a[j][i];
            cout<<b[i][j]<<"\t";
        }
        cout<<"\n";
    }
    
    cout<<"Multiplication of matrix and its transpose:\n";
    int c[m][m];
    for(int i=0;i<m;i++)
    {
        for(int j=0;j<m;i++)
        {
            c[i][j]=0;
            cout<<c[i][j]<<"\t";
        }
        cout<<"\n";
    }
    for (int i=0;i<m;i++)
    {
        for (int j=0;j<m;j++)
        {
            for(int k=0;k<n;k++)
            {
               c[i][j]+=a[i][k]*b[k][j];
               cout<<c[i][j]<<"\t";
            }
        }
        cout<<"\n";
    }
  
}
int main()
{
    int m,n;
    cout<<"enter matrix size:mxn\n";
    cin>>m>>n;
    matrix(m,n);
}
