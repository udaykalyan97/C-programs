#include<iostream>
int main()
{
    int a[10][10],b[10][10],c[10][10],n;
    std::cout<<"enter number of elements:";
    std::cin>>n;
    std::cout<<"enter elements of matrix A:\n";
    for(int i=0;i<n;i++)
    {
        for(int j=0;j<n;j++)
        {
            std::cin>>a[i][j];
        }
    }
    std::cout<<"enter elements of matrix B:\n";
    for(int i=0;i<n;i++)
    {
        for(int j=0;j<n;j++)
        {
            std::cin>>b[i][j];
        }
    }
    std::cout<<"Matrix Multiplication C = A x B\n";
    for(int i=0;i<n;i++)
    {
        for(int j=0;j<n;j++)
        {
            c[i][j]=a[i][j]*b[i][j];
            std::cout<<c[i][j]<<"\t";
        }
        std::cout<<"\n";
    }
}
